---
interface Props {
  label: string;
  countLabel: string;
}

const { label, countLabel } = Astro.props;
---

<div class="section-header">
  <p class="section-header__label">{label}</p>
  <div class="section-header__rule" aria-hidden="true"></div>
  <p class="section-header__count">{countLabel}</p>
</div>
